{
  "source": {
    "include": ["./lib/scripts", "package.json", "README.md"],
    "includePattern": ".js$",
    "excludePattern": "(node_modules/|docs)"
  },
  "plugins": ["plugins/markdown"],
  "opts": {
    "template": "node_modules/clean-jsdoc-theme"
  },
  "markdown": {
    "hardwrap": false,
    "idInHeadings": true
  },
  "theme_opts": {
    "base_url": "https://crabsmp.dev/v1/",
    "homepageTitle": "CrabSMP Dev Docs",
    "favicon": "./icon.png",
    "search": true
  }
}
